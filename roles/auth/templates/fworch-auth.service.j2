[Unit]
Description=FW Orch Auth Server
After=network.target remote-fs.target nss-lookup.target {{ api_service_name }}.service
[Service]
WorkingDirectory={{ auth_server_start_dir }}
ExecStart=/usr/bin/dotnet run FWO_Auth_Server.dll
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier={{ auth_server_syslog_id }}
User={{ fworch_user }}
Environment=
[Install]
WantedBy=multi-user.target
