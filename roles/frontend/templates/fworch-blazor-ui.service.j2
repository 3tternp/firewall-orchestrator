[Unit]
Description={{ product_name }} Blazor UI
After=network.target remote-fs.target nss-lookup.target {{ api_service_name }}.service {{ auth_service_name }}.service
[Service]
WorkingDirectory={{ ui_start_dir }}
ExecStart=/usr/bin/dotnet run FWO.dll
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier={{ ui_syslog_id }}
User={{ fworch_user }}
Environment=
[Install]
WantedBy=multi-user.target
