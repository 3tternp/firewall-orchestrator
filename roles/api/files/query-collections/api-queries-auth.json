
{ "queries": [
    {
        "name": "getTenantId",
        "query": "query getTenantId($tenant_name: String!) { tenant(where: {tenant_name: {_eq: $tenant_name}}) { tenant_id } }"
    },
    {
        "name": "getVisibleDevIds",
        "query": "query getVisibleDevIds($tenant_id: Int!) { device(where: {tenant_to_devices: {tenant_id: {_eq: $tenant_id}}}) { dev_id } }"
    },
    {
        "name": "getVisibleDevIdsFromTenantName",
        "query": "query getVisibleDevIdsFromTenantName($tenant_name: String!) { device(where: {client_to_devices: {tenant_id: {_eq: getTenantId($tenant_name)}}}) { dev_id } }"
    },
    {
        "name": "tenantCanViewAllDevices",
        "query": "query tenantCanViewAllDevices($tenant_id: Int!) { tenant(where:{tenant_id: {_eq: $tenant_id}}) { tenant_can_view_all_devices } }"
    },
    {
        "name": "getLdapConnections",
        "query": "query getLdapConnections { ldap_connection { ldap_server ldap_port ldap_search_user ldap_tls ldap_tenant_level ldap_connection_id ldap_search_user_pwd ldap_searchpath_for_users } }"
    }
]
}